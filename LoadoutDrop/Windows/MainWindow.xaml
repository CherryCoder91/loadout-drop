<Window x:Class="LoadoutDrop.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoadoutDrop"
        xmlns:CustomControls="clr-namespace:LoadoutDrop.CustomControls;assembly=LoadoutDrop.CustomControls"
        xmlns:UserControls="clr-namespace:LoadoutDrop.View.UserControls"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="720"
        Width="1280"
        Icon="../Resources/AppIcons/loadoutdrop.png"
        WindowStyle="None"
        Background="Transparent"
        AllowsTransparency="True"
        MouseDown="Window_MouseDown"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">



    <Grid>
        <CustomControls:WindowPanel Style="{StaticResource PrimaryWindowPanel}">
            <UserControls:AppMenuBar>
            </UserControls:AppMenuBar>
        </CustomControls:WindowPanel>
    </Grid>
    
</Window>
